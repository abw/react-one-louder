{
  "name": "@abw/react-one-louder",
  "version": "0.0.3",
  "description": "Theme provider for React components",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-one-louder.umd.cjs",
  "module": "./dist/react-one-louder.js",
  "exports": {
    ".": {
      "import": "./dist/react-one-louder.js",
      "require": "./dist/react-one-louder.umd.cjs"
    }
  },
  "author": "Andy Wardley",
  "licence": "MIT",
  "keywords": [
    "react",
    "theme",
    "provider"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "vite build --outDir docs --config vite.docs.config.js",
    "preview": "vite preview --outDir docs --config vite.docs.config.js",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abw/react-one-louder.git"
  },
  "bugs": {
    "url": "https://github.com/abw/react-one-louder/issues"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "react-router-dom": "^6.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.60.0",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  }
}
