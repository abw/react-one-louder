{
  "name": "@abw/react-one-louder",
  "version": "0.0.6",
  "description": "Theme provider for React components",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-one-louder.umd.cjs",
  "module": "./dist/react-one-louder.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-one-louder.umd.cjs",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-one-louder.js"
      }
    }
  },
  "author": "Andy Wardley",
  "licence": "MIT",
  "keywords": [
    "react",
    "theme",
    "provider"
  ],
  "scripts": {
    "dev": "vite",
    "build": "$npm_execpath build:lib && $npm_execpath build:docs",
    "build:lib": "tsc -b && vite build",
    "build:docs": "vite build --config vite.docs.js",
    "preview": "vite preview --outDir docs --config vite.docs.config.js",
    "test": "vitest --ui",
    "XXlint": "eslint 'lib/**/*.{js,jsx}' 'src/**/*.{js,jsx}'",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abw/react-one-louder.git"
  },
  "bugs": {
    "url": "https://github.com/abw/react-one-louder/issues"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@abw/badger-timestamp": "^1.2.0",
    "@abw/badger-utils": "1.2.0-ts-beta3",
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^21.1.1",
    "react-router-dom": "^6.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.60.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^2.1.8"
  }
}
